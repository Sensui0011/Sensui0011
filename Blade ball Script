local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({
   Name = "Blade Ball Script",
   HidePremium = false,
   SaveConfig = true,
   ConfigFolder = "Sen hub",
   IntroEnabled = true,
   IntroText = "Made By Sensui",
   IntroIcon = "https://example.com/introicon.png",
   Icon = "https://example.com/windowicon.png",
   CloseCallback = function()
       print("Window has been closed")
   end
})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "nil",
	PremiumOnly = false
})

local Tab = Window:MakeTab({
	Name = "Auto",
	Icon = "nil",
	PremiumOnly = false
})

local Tab = Window:MakeTab({
	Name = "OP",
	Icon = "nil",
	PremiumOnly = false
})

local Tab = Window:MakeTab({
	Name = "Teleport",
	Icon = "nil",
	PremiumOnly = false
})
